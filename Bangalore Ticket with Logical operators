package basics;
//Assignment: 8 
/*	
Male : 	below 2--free
		above 2 and till 12--half ticket
		adult 13 to 60--full ticket
		Sr Citizen 61 -- half ticket
Female: free
*/

public class BangalorebusTicket_LogicalOperators 
{
	public static void main(String[] args)
	 {
	  int age=59;
	  char gender='m'; 
	  
	  if(gender=='f')
	  {
		  System.out.println("female free travel");
		 
	  }	  //end of IF
	  else if((age>=2 && age<=12)|| (age>=13 && age>=60))
	  { 
		  System.out.println("half ticket");
 		   
	  }
	  else
		  System.out.println("full ticket");
	   
	}

}
