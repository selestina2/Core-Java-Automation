package basics;
//Assignment: 7 
/*	
Male : 	below 2--free
		above 2 and till 12--half ticket
		adult 13 to 60--full ticket
		Sr Citizen 61 -- half ticket
Female: free
*/
	public class BangaloreTicket
	{
	 public static void main(String[] args)
	 {
	  int age=13;
	  char gender='m'; 
	  
	  if(gender=='f')
	  {
		  System.out.println("female free travel");
	  }	  //end of IF
	  else 
	  {   
		  if(age>=2)
		   {
		   	   if(age<=12)
			   System.out.println("half ticket");
		   	   else if(age>=13) 
		   	   {
		   		   if(age<=60)
		   			System.out.println("full ticket");
		   		   else
		   			System.out.println("half ticket");
		   	   }
		   		   
		   }
	  }
	   
	 }
	}





